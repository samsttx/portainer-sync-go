# Portainer
PORTAINER_URL=http://localhost:9000
PORTAINER_USER=admin
PORTAINER_PASSWORD=your_password   
PORTAINER_ENDPOINT_ID=1

# Infisical Secrets
INFISICAL_API_URL=https://app.infisical.com
INFISICAL_TOKEN=your_infisical_token
INFISICAL_WORKSPACE_ID=your_infisical_workspace_id
INFISICAL_ENVIRONMENT=your_infisical_environment
INFISICAL_SECRET_PATH=/

# Local Stacks
LOCAL_STACKS_PATH=./stacks

# Set to true to allow overriding stacks managed by Git (e.g., from GitHub)
# When false (default), Git-managed stacks will be skipped during updates
OVERRIDE_MANAGED_STACKS=false

# When true, only shows what would be done without making any changes
DRY_RUN=false